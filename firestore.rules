rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    function isSignedIn() {
      return request.auth != null;
    }
    match /groups/{postId} {
    	allow read: if true;
    }
		match /groups/{postId} {
        allow read, write: if isSignedIn();
    }
    match /{document=**} {
      allow read, write: if false;
    }
  }
}